@article{Perez:2019dm,
author = {Perez, Addiel U and Schmitter-Soto, Juan J and Adams, Aaron J and Heyman, William D},
title = {{Connectivity mediated by seasonal bonefish (\emph{Albula vulpes}) migration between the Caribbean Sea and a tropical estuary of Belize and Mexico}},
journal = {Environmental Biology of Fishes},
year = {2019},
volume = {102},
number = {2},
pages = {197--207},
month = feb,
publisher = {Springer Netherlands},
doi = {10.1007/s10641-018-0834-z},
language = {English},
rating = {0},
date-added = {2020-07-24T18:53:48GMT},
date-modified = {2021-03-19T20:23:02GMT},
abstract = {Bonefish (Albula vulpes) are an important resource for catch-and-release fishing in the Caribbean Sea. Understanding movements within and between the Caribbean Coast (CC) and Chetumal-Corozal Bay (CB) in Mexico and Belize is crucial for identifying and protecting home ranges, migration routes, pre-spawning and spawning sites. We used a mixed-methods approach to document dynamics of bonefish movement. We collected fishers{\textquoteright} local knowledge (LK) using qualitative methods including workshops, key informant interviews, participant observation and field notes about bonefish seasonal movements. We then used mark-recapture (8816 tagged, 569 recaptured) method to understand bonefish movements by size, location and season. Bonefish were significantly larger in CC than in CB. We documented several seasonal movement patterns. A southward movement within CB during the rainy season was likely driven by salinity changes. This was followed by an eastward long-distance migration during the norths or cold front season between the bay and the Caribbean Sea, likely for spawning, as we document likely spawning readiness, pre-spawning behavior and synchronized to the fore-reef at one of two pre-spawning aggregation sites in a World Heritage Site in the CC of Belize during November and December of 2018. There was then a northward movement during the dry season as a journey back to home ranges. The information presented herein can inform resource management and protected areas planning towards a bi-national conservation and management of bonefish and its habitats.},
url = {https://link.springer.com/article/10.1007/s10641-018-0834-z},
uri = {\url{papers3://publication/doi/10.1007/s10641-018-0834-z}}
}

