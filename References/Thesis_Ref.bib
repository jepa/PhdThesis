@article{Woelders:2018kh,
author = {Woelders, Lineke and Lenaerts, Jan T M and Hagemans, Kimberley and Akkerman, Keechy and van Hoof, Thomas B and Hoek, Wim Z},
title = {{Recent climate warming drives ecological change in a remote high-Arctic lake}},
journal = {Scientific Reports},
year = {2018},
volume = {8},
number = {1},
pages = {--8},
publisher = {Nature Publishing Group},
affiliation = {Department of Earth and Environmental Sciences, K.U. Leuven, Celestijnenlaan 200E, 3001, Leuven, Belgium. lineke.woelders@colorado.edu.},
doi = {10.1038/s41598-018-25148-7},
pmid = {29717176},
