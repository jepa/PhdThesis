@article{Free:2019jq,
author = {Free, Christopher M and Thorson, James T and Pinsky, Malin L and Oken, Kiva L and Wiedenmann, John and Jensen, Olaf P},
title = {{Impacts of historical warming on marine fisheries production}},
journal = {Science},
year = {2019},
volume = {363},
number = {6430},
pages = {979--983},
month = mar,
publisher = {American Association for the Advancement of Science},
affiliation = {Department of Marine and Coastal Sciences, Rutgers University, New Brunswick, NJ, USA. cfree14@gmail.com.},
doi = {10.1126/science.aau1758},
pmid = {30819962},
language = {English},
read = {Yes},
rating = {0},
date-added = {2019-07-04T06:55:03GMT},
date-modified = {2020-09-15T13:56:10GMT},
abstract = {Fisheries provide food and support livelihoods across the world. They are also under extreme pressure, with many stocks overfished and poorly managed. Climate change will add to the burden fish stocks bear, but such impacts remain largely unknown. Free et al. used temperature-specific models and hindcasting across fish stocks to determine the degree to which warming has, and will, affect fish species (see the Perspective by Plag{\'a}nyi). They found that an overall reduction in yield has occurred over the past 80 years. Furthermore, although some species are predicted to respond positively to warming waters, the majority will experience a negative impact on growth. As our world warms, responsible and active management of fisheries harvests will become even more important. Science , this issue p. [979][1]; see also p. [930][2] Climate change is altering habitats for marine fishes and invertebrates, but the net effect of these changes on potential food production is unknown. We used temperature-dependent population models to measure the influence of warming on the productivity of 235 populations of 124 species in 38 ecoregions. Some populations responded significantly positively ( n = 9 populations) and others responded significantly negatively ( n = 19 populations) to warming, with the direction and magnitude of the response explained by ecoregion, taxonomy, life history, and exploitation history. Hindcasts indicate that the maximum sustainable yield of the evaluated populations decreased by 4.1% from 1930 to 2010, with five ecoregions experiencing losses of 15 to 35%. Outcomes of fisheries management{\textemdash}including long-term food provisioning{\textemdash}will be improved by accounting for changing productivity in a warmer ocean. [1]: /lookup/doi/10.1126/science.aau1758 [2]: /lookup/doi/10.1126/science.aaw5824},
url = {http://www.sciencemag.org/lookup/doi/10.1126/science.aau1758},
local-url = {file://localhost/Users/carmelia/Dropbox/Papers.Library3.Library/Files/3D/3D9D8A91-214C-4F32-BEC4-6CBA1D8F44AD.pdf},
file = {{3D9D8A91-214C-4F32-BEC4-6CBA1D8F44AD.pdf:/Users/carmelia/Dropbox/Papers.Library3.Library/Files/3D/3D9D8A91-214C-4F32-BEC4-6CBA1D8F44AD.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1126/science.aau1758}}
}

